import { AfterContentInit, ElementRef, OnInit } from '@angular/core';
import { BreakpointObserver } from '@angular/cdk/layout';
import { CollapseDirective } from '../collapse';
export declare class NavbarComponent implements OnInit, AfterContentInit {
    private hostElement;
    private breakpointObserver;
    light: boolean;
    color: string;
    fixed: 'top' | 'bottom';
    sticky: boolean;
    expandable: boolean | string;
    get getClasses(): any;
    get hostClasses(): any;
    get breakpoint(): string;
    collapse: CollapseDirective;
    collapseRef: any;
    constructor(hostElement: ElementRef, breakpointObserver: BreakpointObserver);
    ngOnInit(): void;
    ngAfterContentInit(): void;
}
