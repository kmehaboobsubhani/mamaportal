import { AfterContentChecked, AfterContentInit, ElementRef, EventEmitter, OnChanges, OnDestroy, OnInit, Renderer2, SimpleChanges } from '@angular/core';
import { TabsetService } from './tabset.service';
import { TabComponent } from './tab/tab.component';
export declare class TabsetComponent implements AfterContentChecked, AfterContentInit, OnChanges, OnDestroy, OnInit {
    private hostElement;
    private renderer;
    private tabsetService;
    private _activeTab;
    private _selectedTab;
    private _tabsArray;
    private activeTabSubscription;
    fill: boolean;
    justified: boolean;
    variant: 'tabs' | 'pills';
    fade: boolean;
    vertical: boolean;
    set activeTab(index: number);
    get activeTab(): number;
    boxed: boolean | 'left' | 'right' | 'top-right';
    activeTabChange: EventEmitter<number>;
    private tabList;
    private tabs;
    private panes;
    private host;
    constructor(hostElement: ElementRef, renderer: Renderer2, tabsetService: TabsetService);
    ngOnInit(): void;
    ngOnDestroy(): void;
    ngAfterContentInit(): void;
    ngAfterContentChecked(): void;
    ngOnChanges(changes: SimpleChanges): void;
    setActiveTab(tab?: TabComponent): void;
    private activeTabSubscribe;
    private activeTabUnsubscribe;
}
