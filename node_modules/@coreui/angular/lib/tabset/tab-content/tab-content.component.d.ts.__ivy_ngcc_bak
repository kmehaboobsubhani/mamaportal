import { AfterContentChecked, OnInit, QueryList } from '@angular/core';
import { TabPaneComponent } from '../tab-pane/tab-pane.component';
export declare class TabContentComponent implements OnInit, AfterContentChecked {
    private _hasPanes;
    _tabContent: boolean;
    set hasPanes(hasPanes: boolean);
    get hasPanes(): boolean;
    panes: QueryList<TabPaneComponent>;
    constructor();
    ngOnInit(): void;
    ngAfterContentChecked(): void;
}
