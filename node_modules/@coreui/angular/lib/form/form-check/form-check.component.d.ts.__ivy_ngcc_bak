import { OnInit } from '@angular/core';
export declare class FormCheckComponent implements OnInit {
    custom: boolean;
    disabled: boolean;
    inline: boolean;
    variant: 'checkbox' | 'radio' | 'switch';
    get getClasses(): {
        [x: string]: boolean;
        'form-check': boolean;
        'custom-control': boolean;
        disabled: boolean;
    };
    get hostClasses(): {
        [x: string]: boolean;
        'form-check': boolean;
        'custom-control': boolean;
        disabled: boolean;
    };
    constructor();
    ngOnInit(): void;
}
