import { ElementRef, OnInit } from '@angular/core';
export declare type TInputType = 'button' | 'color' | 'date' | 'datetime' | 'datetime-local' | 'email' | 'file' | 'hidden' | 'image' | 'month' | 'number' | 'password' | 'range' | 'reset' | 'search' | 'submit' | 'tel' | 'text' | 'time' | 'url' | 'week' | 'checkbox' | 'radio';
export declare class InputDirective implements OnInit {
    private hostElement;
    private host;
    valid: boolean;
    plaintext: boolean;
    sizing: '' | 'sm' | 'lg' | string;
    custom: boolean;
    type: TInputType;
    get getClasses(): any;
    get hostClasses(): any;
    get isPlainText(): boolean;
    get isFormControl(): boolean;
    get isFormCheck(): boolean;
    constructor(hostElement: ElementRef);
    ngOnInit(): void;
    get attributes(): string[];
    get isReadonly(): boolean;
    get isDisabled(): boolean;
}
