import { OnInit } from '@angular/core';
export declare class InputGroupComponent implements OnInit {
    size: string | 'sm' | 'lg' | '';
    get getClasses(): {
        [x: string]: boolean;
        'input-group': boolean;
    };
    get hostClasses(): {
        [x: string]: boolean;
        'input-group': boolean;
    };
    constructor();
    ngOnInit(): void;
}
