import { OnInit } from '@angular/core';
export declare class InputGroupAddonComponent implements OnInit {
    prepend: boolean;
    get getClasses(): {
        [x: string]: boolean;
    };
    get hostClasses(): {
        [x: string]: boolean;
    };
    constructor();
    ngOnInit(): void;
}
export declare class InputGroupPrependComponent extends InputGroupAddonComponent {
    prepend: boolean;
}
export declare class InputGroupAppendComponent extends InputGroupAddonComponent {
    prepend: boolean;
}
