import { OnDestroy, ElementRef, TemplateRef, ChangeDetectorRef, OnInit, Renderer2 } from '@angular/core';
export declare class ModalComponent implements OnDestroy, OnInit {
    private document;
    private renderer;
    private elementRef;
    private changeDetectorRef;
    header: TemplateRef<any>;
    body: TemplateRef<any>;
    footer: TemplateRef<any>;
    closeOnBackdrop: boolean;
    size: 'sm' | '' | 'lg' | 'xl';
    color: string;
    modalClasses: {
        modal: boolean;
        fade: boolean;
    };
    dialogClasses: {
        'modal-dialog': boolean;
    };
    backdropClasses: {
        'modal-backdrop': boolean;
        fade: boolean;
        show: boolean;
    };
    visible: boolean;
    visibleAnimate: boolean;
    constructor(document: any, renderer: Renderer2, elementRef: ElementRef, changeDetectorRef: ChangeDetectorRef);
    ngOnInit(): void;
    ngOnDestroy(): void;
    setClasses(): void;
    show(): void;
    hide(): void;
    onContainerClicked(event: MouseEvent): void;
    onKeyDownHandler(event: KeyboardEvent): void;
    isTopmost(): boolean;
}
