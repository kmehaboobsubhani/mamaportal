import { AfterViewInit, ChangeDetectorRef, ElementRef, OnInit } from '@angular/core';
import { CarouselState } from '../carousel-state';
export declare class CarouselControlComponent implements OnInit, AfterViewInit {
    private changeDetector;
    private carouselState;
    direction: 'prev' | 'next';
    content: ElementRef;
    hasContent: boolean;
    get hostRole(): string;
    get getClasses(): any;
    get carouselControlClass(): string;
    get carouselControlIconClass(): string;
    get hostClasses(): any;
    onKeyUp($event: KeyboardEvent): void;
    onClick($event: MouseEvent): void;
    constructor(changeDetector: ChangeDetectorRef, carouselState: CarouselState);
    ngOnInit(): void;
    ngAfterViewInit(): void;
    private play;
}
