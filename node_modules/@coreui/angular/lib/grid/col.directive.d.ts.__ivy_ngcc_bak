interface BpObject {
    size?: number | string;
    offset?: number | string;
    order?: number | string;
}
declare type BreakPoint = (string | number | BpObject);
export declare class ColDirective {
    xs: BreakPoint;
    sm: BreakPoint;
    md: BreakPoint;
    lg: BreakPoint;
    xl: BreakPoint;
    xxl: BreakPoint;
    col: BreakPoint;
    get getClasses(): string;
    private readonly suffix;
    get hostClasses(): string;
}
export {};
