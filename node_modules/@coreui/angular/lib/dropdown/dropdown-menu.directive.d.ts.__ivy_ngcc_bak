import { ElementRef, OnDestroy, OnInit, Renderer2 } from '@angular/core';
import { DropdownState } from './dropdown.state';
import { DropdownService } from './dropdown.service';
export declare class DropdownMenuDirective implements OnInit, OnDestroy {
    private renderer;
    private hostElement;
    private dropdownState;
    private dropdownService;
    role: string;
    dropdownMenu: boolean;
    private stateToggleSubscription;
    private readonly host;
    constructor(renderer: Renderer2, hostElement: ElementRef, dropdownState: DropdownState, dropdownService: DropdownService);
    ngOnInit(): void;
    ngOnDestroy(): void;
    show(show: any): void;
    align(align: any): void;
    private stateToggleSubscribe;
}
