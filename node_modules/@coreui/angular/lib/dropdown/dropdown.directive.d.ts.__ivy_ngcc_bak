import { AfterViewInit, ElementRef, OnChanges, OnDestroy, OnInit, Renderer2, SimpleChanges } from '@angular/core';
import { OutClickService } from '../shared';
import { DropdownService } from './dropdown.service';
import { DropdownState } from './dropdown.state';
export declare class DropdownDirective implements OnInit, OnChanges, OnDestroy, AfterViewInit {
    private renderer;
    private hostElement;
    private outClickService;
    private dropdownService;
    private dropdownState;
    direction: 'down' | 'left' | 'right' | 'up';
    cDropdownAlign: '' | 'right';
    private stateToggleSubscription;
    private outClickSubscription;
    private state;
    cDropdownShow: boolean;
    constructor(renderer: Renderer2, hostElement: ElementRef, outClickService: OutClickService, dropdownService: DropdownService, dropdownState: DropdownState);
    ngOnInit(): void;
    ngOnDestroy(): void;
    ngOnChanges(changes: SimpleChanges): void;
    ngAfterViewInit(): void;
    private stateToggleSubscribe;
    show(show: any): void;
    toggle(): void;
    private outClickSubscribe;
    autoClose(e: any): void;
}
