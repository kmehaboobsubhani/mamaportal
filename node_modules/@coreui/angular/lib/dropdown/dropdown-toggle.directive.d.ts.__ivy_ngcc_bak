import { ElementRef, OnInit, Renderer2 } from '@angular/core';
import { DropdownState } from './dropdown.state';
import { DropdownService } from './dropdown.service';
import { DropdownDirective } from './dropdown.directive';
export declare class DropdownToggleDirective implements OnInit {
    private renderer;
    private hostElement;
    private dropdown;
    private dropdownService;
    private dropdownState;
    cDropdownCaret: 'caret' | 'split' | 'none';
    private state;
    private readonly host;
    constructor(renderer: Renderer2, hostElement: ElementRef, dropdown: DropdownDirective, dropdownService: DropdownService, dropdownState: DropdownState);
    ngOnInit(): void;
    toggleShow($event: any): void;
}
