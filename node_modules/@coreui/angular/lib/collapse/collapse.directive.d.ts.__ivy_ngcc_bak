import { DoCheck, ElementRef, EventEmitter, OnChanges, OnDestroy, OnInit, SimpleChanges } from '@angular/core';
import { AnimationBuilder } from '@angular/animations';
export declare class CollapseDirective implements OnInit, OnChanges, OnDestroy, DoCheck {
    private hostElement;
    private animationBuilder;
    navbar: boolean;
    duration: string;
    transition: string;
    show: boolean;
    animate: boolean;
    collapseChange: EventEmitter<string>;
    private _show;
    _animate: boolean;
    private player;
    private readonly host;
    get getClasses(): any;
    get hostClasses(): any;
    constructor(hostElement: ElementRef, animationBuilder: AnimationBuilder);
    ngOnInit(): void;
    ngOnDestroy(): void;
    ngOnChanges(changes: SimpleChanges): void;
    ngDoCheck(): void;
    toggle(show?: boolean): void;
    destroyPlayer(): void;
    createPlayer(show?: boolean): void;
}
