import { ElementRef, ChangeDetectorRef, AfterViewInit } from '@angular/core';
export declare class TogglerComponent implements AfterViewInit {
    private changeDetector;
    inHeader: boolean;
    inNavbar: boolean;
    displayFlex: boolean;
    content: ElementRef;
    hasContent: boolean;
    constructor(changeDetector: ChangeDetectorRef);
    get buttonClass(): "c-header-toggler" | "navbar-toggler";
    get iconClass(): "c-header-toggler-icon" | "navbar-toggler-icon";
    ngAfterViewInit(): void;
}
