import { ElementRef, OnDestroy, OnInit } from '@angular/core';
import { OutClickService } from './out-click.service';
export declare class OutClickDirective implements OnInit, OnDestroy {
    private elementRef;
    private outClickService;
    private outClickSubscription;
    constructor(elementRef: ElementRef, outClickService: OutClickService);
    onDocumentClick($event: MouseEvent): void;
    ngOnInit(): void;
    ngOnDestroy(): void;
}
