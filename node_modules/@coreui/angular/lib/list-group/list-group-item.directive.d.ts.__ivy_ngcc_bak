import { ElementRef, OnChanges, OnInit, Renderer2, SimpleChanges } from '@angular/core';
export declare class ListGroupItemDirective implements OnChanges, OnInit {
    private renderer;
    private hostElement;
    accent: null | string;
    action: null | boolean;
    active: null | boolean;
    color: null | string;
    divider: null | boolean;
    disabled: null | string | boolean;
    get getClasses(): any;
    get hostClasses(): any;
    host: HTMLElement;
    constructor(renderer: Renderer2, hostElement: ElementRef);
    ngOnInit(): void;
    ngOnChanges(changes: SimpleChanges): void;
}
