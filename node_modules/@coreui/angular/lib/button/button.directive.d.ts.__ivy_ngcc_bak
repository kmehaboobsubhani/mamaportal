import { OnInit } from '@angular/core';
export declare class ButtonDirective implements OnInit {
    block: boolean | null;
    shape: 'square' | 'pill';
    variant: 'ghost' | 'outline';
    size: 'sm' | 'lg' | '';
    color: string;
    type: 'button' | 'submit' | 'reset';
    tabindex: string | null;
    get getClasses(): any;
    get hostClasses(): any;
    disabled: boolean | string | null;
    get isDisabled(): boolean;
    pressed: boolean | string | null;
    get getTabindex(): string;
    get hostType(): string;
    get hostRole(): string;
    constructor();
    ngOnInit(): void;
}
