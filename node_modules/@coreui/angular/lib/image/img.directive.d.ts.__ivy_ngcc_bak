import { OnInit } from '@angular/core';
export declare class ImgDirective implements OnInit {
    block: boolean;
    fluid: boolean;
    fluidGrow: boolean;
    shape: string | 'rounded' | 'rounded-top' | 'rounded-right' | 'rounded-bottom' | 'rounded-left' | 'rounded-circle' | 'rounded-pill' | 'rounded-0' | 'rounded-sm' | 'rounded-lg';
    thumbnail: boolean;
    align: '' | 'left' | 'right' | 'center';
    placeholderColor: string;
    get getStyles(): {
        backgroundColor: string;
    };
    get getClasses(): any;
    get hostClasses(): any;
    get alignClass(): "" | "mx-auto" | "float-right" | "float-left";
    constructor();
    ngOnInit(): void;
}
