import { Pipe } from '@angular/core';
export class SidebarNavIconPipe {
    transform(item, args) {
        const classes = {
            'c-sidebar-nav-icon': true
        };
        const icon = item.icon;
        classes[icon] = !!item.icon;
        return classes;
    }
}
SidebarNavIconPipe.decorators = [
    { type: Pipe, args: [{
                name: 'cSidebarNavIcon'
            },] }
];
