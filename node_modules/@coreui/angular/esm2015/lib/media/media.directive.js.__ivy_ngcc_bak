import { Directive, HostBinding } from '@angular/core';
export class MediaDirective {
    constructor() {
        this.madiaClass = true;
    }
}
MediaDirective.decorators = [
    { type: Directive, args: [{
                selector: '[cMedia]',
            },] }
];
MediaDirective.propDecorators = {
    madiaClass: [{ type: HostBinding, args: ['class.media',] }]
};
