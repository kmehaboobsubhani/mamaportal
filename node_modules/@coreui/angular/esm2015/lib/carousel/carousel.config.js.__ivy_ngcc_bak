import { Injectable } from '@angular/core';
export class CarouselConfig {
    constructor() {
        this.activeIndex = 0;
        this.animate = true;
        this.direction = 'next';
        this.interval = 3000;
    }
}
CarouselConfig.decorators = [
    { type: Injectable }
];
