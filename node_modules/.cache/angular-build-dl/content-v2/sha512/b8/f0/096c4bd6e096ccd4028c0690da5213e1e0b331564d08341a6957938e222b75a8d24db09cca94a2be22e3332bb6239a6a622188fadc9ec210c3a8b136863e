            this.pepsiPercentage = this.pepsi / (this.bhujiaSev + this.pepsi + this.chips + this.sanitaryNapkinSmall) * 100;
            this.chipsPercentage = this.chips / (this.bhujiaSev + this.pepsi + this.chips + this.sanitaryNapkinSmall) * 100;
