fsetParent);\r\n      offsets.x += offsetParent.clientLeft;\r\n      offsets.y += offsetParent.clientTop;\r\n    } else if (documentElement) {\r\n      offsets.x = getWindowScrollBarX(documentElement);\r\n    }\r\n  }\r\n\r\n  return {\r\n    x: rect.left + scroll.scrollLeft - offsets.x,\r\n    y: rect.top + scroll.scrollTop - offsets.y,\r\n    width: rect.width,\r\n    height: rect.height\r\n  };\r\n}","import { Component, OnInit } from '@angular/core';\r\n\r\n@Component({\r\n  selector: 'app-jumbotrons',\r\n  templateUrl: './jumbotrons.component.html',\r\n  styleUrls: ['./jumbotrons.component.scss']\r\n})\r\nexport class JumbotronsComponent implements OnInit {\r\n\r\n  constructor() { }\r\n\r\n  ngOnInit(): void {\r\n  }\r\n\r\n}\r\n","export default \"<div class=\\\"fade-in\\\">\\r\\n  <c-row>\\r\\n    <c-col sm=\\\"12\\\" md=\\\"6\\\">\\r\\n      <c-card>\\r\\n        <c-card-header>\\r\\n          Collapse\\r\\n        </c-card-header>\\r\\n        <c-card-body>\\r\\n          <p>\\r\\n            <button cButton color=\\\"primary\\\" (click)=\\\"toggleCollapse(0)\\\">\\r\\n              Toggle collapse\\r\\n            </button>\\r\\n          </p>\\r\\n          <c-card cCollapse [show]=\\\"collapses[0]\\\" >\\r\\n            <c-card-body>\\r\\n              {{loremText}}\\r\\n            </c-card-body>\\r\\n          </c-card>\\r\\n        </c-card-body>\\r\\n      </c-card>\\r\\n      <c-card>\\r\\n        <c-card-header>\\r\\n          Collapse <small>multiple targets</small>\\r\\n        </c-card-header>\\r\\n        <c-card-body>\\r\\n          <p>\\r\\n            <button cButton color=\\\"primary\\\" (click)=\\\"toggleCollapse(1)\\\" class=\\\"mr-1\\\">\\r\\n              Toggle first\\r\\n            </button>\\r\\n            <button cButton color=\\\"primary\\\" (click)=\\\"toggleCollapse(2)\\\" class=\\\"mr-1\\\">\\r\\n              Toggle second\\r\\n            </button>\\r\\n            <button cButton color=\\\"primary\\\" (click)=\\\"toggleCol