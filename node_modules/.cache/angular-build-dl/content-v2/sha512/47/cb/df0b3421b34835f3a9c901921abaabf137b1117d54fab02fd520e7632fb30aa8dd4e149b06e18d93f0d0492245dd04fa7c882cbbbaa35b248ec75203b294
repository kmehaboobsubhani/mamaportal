    /** @type {?} */
    const maxBound = max && Object(ngx_bootstrap_chronos__WEBPACK_IMPORTED_MODULE_2__["isAfter"])(Object(ngx_bootstrap_chronos__WEBPACK_IMPORTED_MODULE_2__["startOf"])(date, 'year'), max, 'day');
    return minBound || maxBound;
}
